{
  "name": "mongofn",
  "version": "0.7.1",
  "description": "A practical library to provide you functional mongodb development.",
  "scripts": {
    "lint": "eslint -c ./.eslintrc.js --ext js ./ --fix",
    "clean": "rimraf es/* dist/*",
    "build:es": "cross-env BABEL_ENV=es babel ./src --out-dir ./es",
    "build:cjs": "babel ./src --out-dir ./dist",
    "build": "npm run build:es && npm run build:cjs",
    "precommit": "npm run lint",
    "prepublishOnly": "npm run precommit && npm run clean && npm run build",
    "postpublish": "npm run clean",
    "test": "jest"
  },
  "main": "./dist/index.js",
  "module": "./es/index.js",
  "files": [
    "es",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmetegesel/mongofn.git"
  },
  "dependencies": {
    "ramda": "^0.27.0",
    "mongodb": "^3.5.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-throw-expressions": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-import": "^2.22.0",
    "babel-eslint": "^10.1.0",
    "@types/ramda": "^0.27.6",
    "rimraf": "^3.0.2",
    "cross-env": "^7.0.2",
    "jest": "^26.4.2",
    "babel-jest": "^26.3.0",
    "@types/jest": "^26.0.12"
  },
  "keywords": [
    "mongofn",
    "mongodb",
    "nodejs",
    "functional-programming",
    "fp",
    "ramda"
  ],
  "author": "Ahmet Egesel <ahmetegesel@hotmail.com>",
  "license": "MIT"
}
