{
  "name": "mongofn",
  "version": "0.3.1",
  "description": "A practical library to provide you functional mongodb development.",
  "scripts": {
    "lint": "eslint -c ./.eslintrc.js --ext js ./ --fix",
    "clean": "rimraf es/* dist/*",
    "build:es": "babel . --out-dir ./es --ignore node_modules typings",
    "build:cjs": "babel . --out-dir ./dist --ignore node_modules typings",
    "build": "npm run build:es && npm run build:cjs",
    "precommit": "npm run lint",
    "prepare": "npm run precommit && npm run clean && npm run build"
  },
  "main": "./dist/index.js",
  "module": "./es/index.js",
  "exports": {
    ".": {
      "require": "./src/index.js",
      "import": "./es/index.js",
      "default": "./src/index.js"
    },
    "./es/": "./es/",
    "./src/": "./src/",
    "./dist/": "./dist/"
  },
  "files": [
    "es",
    "dist",
    "src",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com/ahmetegesel/functional-mongo.git"
  },
  "dependencies": {
    "ramda": "^0.27.0",
    "mongodb": "^3.5.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-throw-expressions": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-import": "^2.22.0",
    "babel-eslint": "^10.1.0",
    "@types/ramda": "^0.27.6",
    "rimraf": "^3.0.2"
  },
  "keywords": [
    "mongofn",
    "mongodb",
    "nodejs",
    "functional-programming",
    "fp",
    "ramda"
  ],
  "author": "Ahmet Egesel <ahmetegesel@hotmail.com>",
  "license": "MIT"
}
